@model IEnumerable<ITAssetsDatabase.Models.Upload>

@{
    ViewBag.Title = "Upload";
}

<h2>Upload</h2>

<p>
    @Html.ActionLink("Add Application", "UploadAdd")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InstallLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StaffId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Packaged)
