            @Html.DisplayFor(modelItem => item.Staff.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Packaged)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UploadedFiles)

                    </td>
        <td>
